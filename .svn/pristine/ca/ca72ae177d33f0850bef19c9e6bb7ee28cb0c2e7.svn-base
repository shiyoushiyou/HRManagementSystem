#端口號
server.port=8000
#上下文路徑
server.servlet.context-path=/hrms

#數據庫連結
spring.datasource.url=jdbc:mysql://localhost:3306/mysql
spring.datasource.username=root
spring.datasource.password=wq
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

#mybatis配置
#指定mybatis中SQL的XML的文件路徑
mybatis.mapper-locations=classpath:jp/co/hrms/mapper/*.xml
mybatis.type-aliases-package=jp.co.hrms.model
mybatis.configuration.map-underscore-to-camel-case=true

#Thymeleaf配置
#视图定位
spring.thymeleaf.prefix=classpath:/templates/view/
spring.thymeleaf.suffix=.html

#不加载缓存
spring.thymeleaf.cache=false
spring.thymeleaf.mode=HTML
spring.thymeleaf.encoding=UTF-8
spring.thymeleaf.servlet.content-type=text/html; charset=utf-8

logging.level.org.springframework=WARN
logging.level.jp.co.hrms.mapper=DEBUG
